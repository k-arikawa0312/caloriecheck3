generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Menu {
  id        Int      @id @default(autoincrement())
  menuTitle String
  amount    Float
  ateAt     DateTime
  timeZone  String
  calories  Float
  protein   Float
  fat       Float
  carbs     Float
  fiber     Float
  salt      Float
}

model Ingredient {
  id         Int    @id @default(autoincrement())
  ingredient String
  amount     String
}
